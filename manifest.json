{
    "name": "MLB Calc",
    "version": "0.1.0",
    "description": "A calcular of product cost, fee and profit",
    "manifest_version": 3,
    "icons//": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "background":{
        "service_worker": "./background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "defaut_icon//": {
                "16": "icons/icon-16.png",
                "48": "icons/icon-48.png",
                "128": "icons/icon-128.png"
            } 
    },
    "options_page": "./options.html",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://www.google.com/",
        "<all_urls>",
        "*://*/*"
    ]

}